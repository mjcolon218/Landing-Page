   <button onclick="playStuccInTheGrind()">Play "Stucc in the Grind" by Nipsey Hussle</button>
   <div id="musicPlayer"></div>
 
   <script>
     function playStuccInTheGrind() {
       var searchQuery = "Nipsey Hussle Stucc in the Grind";
 
       $.ajax({
         url: "https://itunes.apple.com/search",
         data: {
           term: searchQuery,
           media: "music",
           limit: 1
         },
         dataType: "jsonp",
         success: function(response) {
           var track = response.results[0];
           var previewUrl = track.previewUrl;
 
           var audioPlayer = document.createElement("audio");
           audioPlayer.src = previewUrl;
           audioPlayer.controls = true;
           audioPlayer.autoplay = true;
 
           $("#musicPlayer").empty().append(audioPlayer);
         },
         error: function(error) {
           console.log("Error: " + error);
         }
       });
     }
   </script>
